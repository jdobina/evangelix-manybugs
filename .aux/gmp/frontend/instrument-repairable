#!/bin/bash

file="$1"

sed -i 's/tn \* 2 - 1/angelix_trace_and_load(tn * 2 - 1, 170, 9, 170, 19)/' "$file"
sed -i 's/b2p = tp + 2\*n;/b2p = angelix_trace_and_load(tp + 2*n, 212, 8, 212, 16);/' "$file"
sed -i 's/ebi < windowsize/angelix_trace_and_load(ebi < windowsize, 247, 6, 247, 22)/g' "$file"
sed -i 's/REDC_1_TO_REDC_2_THRESHOLD < MUL_TOOM22_THRESHOLD/angelix_trace_and_load(REDC_1_TO_REDC_2_THRESHOLD < MUL_TOOM22_THRESHOLD, 303, 6, 303, 55)/g' "$file"
